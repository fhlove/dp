#include<iostream>
#include<cstdio>
using namespace std;
const int N=105;
const int inf=999999;
int dp[N][1<<9][N][10];
int h[N];
void init(int n,int m)
{
    for(int i=0;i<=n;i++)
        for(int s=0;s<(1<<8);s++)
            for(int j=0;j<=m;j++)
                for(int k=0;k<=8;k++)
                    dp[i][s][j][k]=inf;
}
int main()
{
    freopen("input.txt","r",stdin);
    int n,m;
    while(cin>>n>>m&&(n||m))
    {
        init(n,m);
        for(int i=1;i<=n;i++)
            cin>>h[i],h[i]-=25;

        for(int i=1;i<=n;i++)
        {

            dp[i][1<<h[i]][i-1][h[i]]=1;
            //cout<<"------------"<<endl;
            for(int s=0;s<(1<<8);s++)
            {

                for(int j=0;j<m&&j<i;j++)
                {
                    for(int k=0;k<8;k++)
                    {

                        if(dp[i-1][s][j][k]!=inf)
                        {

                            if(j<m) dp[i][s][j+1][k]=min(dp[i][s][j+1][k],dp[i-1][s][j][k]);

                            if(k==h[i]) dp[i][s][j][k]=min(dp[i][s][j][k],dp[i-1][s][j][k]);
                            else dp[i][s|(1<<h[i])][j][k]=min(dp[i-1][s|(1<<h[i])][j][h[i]],dp[i-1][s][j][k]+1);
                        }
                    }
                }
            }
        }
        int ans=inf;
        for(int s=0;s<(1<<8);s++)
        {
            for(int k=0;k<8;k++)
            {
                ans=min(ans,dp[n][s][m][k]);
            }
        }
        cout<<ans<<endl;
    }
    return 0;
}
